{
	"name": "@l1network/chaingraph-genql",
	"version": "0.2.0",
	"type": "module",
	"description": "chaingraph genql",
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"gen": "genql --endpoint https://chaingraph.app/v1/graphql --output ./generated -H 'content-type: application/json' -H 'x-hasura-admin-secret: xxx'",
		"build": "tsup src/index.ts --format esm,cjs --dts --external react",
		"dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
		"lint": "eslint \"src/**/*.ts*\"",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"license": "MIT",
	"dependencies": {
		"@genql/runtime": "^2.10.0",
		"graphql": "^16.10.0",
		"graphql-ws": "^6.0.4"
	},
	"devDependencies": {
		"@chaingraph/tsconfig": "workspace:*",
		"@genql/cli": "^6.3.3",
		"genql": "^2.0.1",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	}
}